
@{
    ViewBag.Title = "ListUser";
}


<div class="col-md-8 col-md-offset-3">
    <form class="form-horizontal">
        <fieldset>
            <legend>Search places</legend>
            <div class="form-group">
                <label for="inputEmail" class="col-lg-2 control-label">Category</label>
                <div class="col-lg-10">
                    <input class="form-control" id="inputEmail" placeholder="Category" type="text">
                </div>
            </div>
            <div class="form-group">
                <label for="inputPassword" class="col-lg-2 control-label">City</label>
                <div class="col-lg-10">
                    <input class="form-control" id="inputPassword" placeholder="City" type="text">
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-2 control-label">Radios</label>
                <div class="col-lg-10">
                    <div class="radio col-md-3">
                        <label>
                            <input name="optionsRadios" id="optionsRadios1" value="option1" type="radio">
                            List all places
                        </label>
                    </div>
                    <div class="radio col-md-3">
                        <label>
                            <input name="optionsRadios" id="optionsRadios2" value="option2" type="radio">
                            List map places
                        </label>
                    </div>
                </div>
            </div>
            <div class="form-group col-md-offset-5">
                <div class="col-lg-10 col-lg-offset-2">
                    <button type="submit" class="btn btn-primary">Submit</button>
                </div>
            </div>
        </fieldset>
    </form>
</div>

